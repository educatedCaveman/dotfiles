#aliases:
alias psgrep='ps -e | grep'
alias vpnstop='sudo systemctl stop openvpn@NYC.service'
alias vpnstart='sudo systemctl start openvpn@NYC.service'
alias vpnstatus='systemctl status openvpn@NYC.service'
alias vpnkill='sudo killall openvpn'
alias nautilus='nohup nautilus --no-desktop &>/dev/null &'
alias vim='/home/drake/scripts/neovim_reminder.sh'
alias please='sudo $( history -p !!)'
alias fuck='sudo $( history -p !!)'
alias fucking='sudo'
alias ll='ls -lhA'

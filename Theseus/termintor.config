[plugins]
[profiles]
  [[default]]
    background_color = "#2b2b2b"
    cursor_color = "#dedede"
    foreground_color = "#dedede"
    scrollback_infinite = True
    show_titlebar = False
  [[htop]]
    background_color = "#2b2b2b"
    cursor_color = "#aaaaaa"
    custom_command = 'bash -i -c "htop && bash"'
    foreground_color = "#dedede"
    scrollbar_position = hidden
    show_titlebar = False
    use_custom_command = True
  [[scratchpad]]
    background_color = "#2b2b2b"
    cursor_color = "#aaaaaa"
    foreground_color = "#dedede"
    show_titlebar = False
  [[speedometer-down]]
    background_color = "#2b2b2b"
    cursor_color = "#aaaaaa"
    custom_command = 'bash -i -c "speedometer -r eno1 && bash"'
    foreground_color = "#dedede"
    scrollbar_position = hidden
    show_titlebar = False
    use_custom_command = True
  [[speedometer-up]]
    background_color = "#2b2b2b"
    cursor_color = "#aaaaaa"
    custom_command = 'bash -i -c "speedometer -t eno1 && bash"'
    foreground_color = "#dedede"
    scrollbar_position = hidden
    show_titlebar = False
    use_custom_command = True

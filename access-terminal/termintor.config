      uuid = 9381c2b0-3f37-497b-aafc-5533762b4393
[plugins]
[profiles]
  [[default]]
    background_color = "#2b2b2b"
    cursor_color = "#dedede"
    font = Hack 11
    foreground_color = "#dedede"
    scrollback_infinite = True
    show_titlebar = False
    use_system_font = False
  [[apprentice]]
    background_color = "#262626"
    cursor_color = "#aaaaaa"
    foreground_color = "#bcbcbc"
    palette = "#1c1c1c:#af5f5f:#5f875f:#87875f:#5f87af:#5f5f87:#5f8787:#6c6c6c:#444444:#ff8700:#87af87:#ffffaf:#8fafd7:#8787af:#5fafaf:#ffffff"
    show_titlebar = False
  [[htop]]
    background_color = "#2b2b2b"
    custom_command = 'bash -i -c "htop && bash"'
    font = Hack 11
    foreground_color = "#dedede"
    scrollbar_position = hidden
    show_titlebar = False
    use_custom_command = True
    use_system_font = False
  [[speedometer]]
    background_color = "#2b2b2b"
    custom_command = 'bash -i -c "speedometer -t tun0 -r tun0 -t wlp2s0 -r wlp2s0 && bash"'
    font = Hack 11
    foreground_color = "#dedede"
    scrollbar_position = hidden
    show_titlebar = False
    use_custom_command = True
    use_system_font = False

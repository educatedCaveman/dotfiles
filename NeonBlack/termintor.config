[global_config]
  inactive_color_offset = 1.0
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#2b2b2b"
    background_image = None
    foreground_color = "#dedede"
    scrollback_infinite = True
    show_titlebar = False
  [[htop]]
    background_color = "#2b2b2b"
    background_image = None
    custom_command = 'bash -i -c "htop && bash"'
    foreground_color = "#dedede"
    show_titlebar = False
    use_custom_command = True
  [[speedometer-eno1]]
    background_color = "#2b2b2b"
    background_image = None
    custom_command = 'bash -i -c "speedometer -t eno1 -r eno1 && bash"'
    foreground_color = "#dedede"
    show_titlebar = False
    use_custom_command = True
  [[speedometer-tun0]]
    background_color = "#2b2b2b"
    background_image = None
    custom_command = 'bash -i -c "speedometer -t tun0 -r tun0 && bash"'
    foreground_color = "#dedede"
    show_titlebar = False
    use_custom_command = True

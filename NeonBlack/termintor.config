[plugins]
[profiles]
  [[default]]
    background_color = "#2b2b2b"
    cursor_color = "#dedede"
    foreground_color = "#dedede"
    scrollback_infinite = True
    show_titlebar = False
  [[htop]]
    background_color = "#2b2b2b"
    cursor_color = "#dedede"
    custom_command = 'bash -i -c "htop && bash"'
    foreground_color = "#dedede"
    scrollbar_position = hidden
    show_titlebar = False
    use_custom_command = True
  [[speedometer-eno1]]
    background_color = "#2b2b2b"
    cursor_color = "#dedede"
    custom_command = 'bash -i -c "speedometer -t eno1 -r eno1 && bash"'

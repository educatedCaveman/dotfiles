[global_config]
  inactive_color_offset = 1.0
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    background_color = "#2b2b2b"
    cursor_color = "#aaaaaa"
    foreground_color = "#dedede"
    scrollback_infinite = True
    show_titlebar = False
  [[command]]
    background_color = "#2b2b2b"
    cursor_color = "#aaaaaa"
    foreground_color = "#dedede"
    scrollbar_position = hidden
    show_titlebar = False
